# .env - Sample environment variables for Agentic DevOps Assistant
# AWS credentials (if not using AWS CLI profile)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_DEFAULT_REGION=us-east-1

# GitHub token (if using GitHub integration in future phases)
GITHUB_TOKEN=your_github_token

# CDK project configuration (optional overrides)
CDK_PROJECT_NAME=my-generated-api
CDK_AWS_REGION=us-east-1
CDK_AWS_ACCOUNT_ID=123456789012
CDK_ENV_NAME=dev
